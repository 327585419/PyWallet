#:import MDList kivymd.list.MDList


<SwitchAccount>:
    orientation: 'vertical'
    ScrollView:
        MDList:
            id: account_list_id


<SwitchAccountScreen>:
    name: 'switch_account'
    on_pre_enter:
        app.controller.set_toolbar_title("Switch account")
        switch_account_id.load_account_list()
    SwitchAccount:
        id: switch_account_id
